#.env.example
# API Configuration
OPENAI_API_KEY=your_openai_api_key_here

GITHUB_PERSONAL_ACCESS_TOKEN=your_github_personal_access_token

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Database Configuration
DATABASE_URL=sqlite:///data/database/agentic_workflow.db
DATABASE_POOL_SIZE=10
DATABASE_ECHO=false

# GitHub MCP Server Configuration
GITHUB_MCP_SERVER_URL=http://localhost:3000
GITHUB_MCP_SERVER_TOKEN=your_mcp_server_token

# Application Configuration
APP_NAME=Agentic AI Workflow
APP_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# Repository Processing
TEMP_REPO_PATH=data/temp/repositories
MAX_REPO_SIZE_MB=500
CLONE_TIMEOUT_SECONDS=300

# Rate Limiting
OPENAI_REQUESTS_PER_MINUTE=50
GITHUB_REQUESTS_PER_HOUR=5000

# Context Window
MAX_CONTEXT_LENGTH=32000
MAX_FILES_PER_ANALYSIS=100